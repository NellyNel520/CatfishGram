{
  "expo": {
    "name": "CatfishGram",
    "slug": "catfishgram",
    "description": "Instagram Clone built with react native expo, firebase authentication and fireStore database. Replicating the real Instagram's functionality and design.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/catfishGram-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/catfishGram-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/catfishGram-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/catfishGram-icon.png"
    }
  }
}
